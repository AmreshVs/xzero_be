{
  "kind": "collectionType",
  "collectionName": "center_check_ins",
  "info": {
    "name": "Center Check-in"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user_id": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "center_check_ins"
    },
    "center": {
      "model": "centers"
    },
    "membership": {
      "model": "membership",
      "via": "center_check_ins"
    },
    "transaction_id": {
      "type": "string"
    },
    "discounted_price": {
      "type": "integer",
      "default": 0
    },
    "original_price": {
      "type": "integer"
    },
    "discount": {
      "type": "integer"
    },
    "offer_id": {
      "model": "offers",
      "via": "center_check_ins"
    }
  }
}
