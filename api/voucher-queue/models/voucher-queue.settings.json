{
  "kind": "collectionType",
  "collectionName": "voucher_queues",
  "info": {
    "name": "Voucher Queue"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "voucher": {
      "model": "vouchers"
    },
    "status": {
      "type": "boolean",
      "default": true
    },
    "user_status": {
      "type": "string"
    },
    "is_notified": {
      "type": "boolean"
    }
  }
}
