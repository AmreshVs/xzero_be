{
  "kind": "collectionType",
  "collectionName": "memberships",
  "info": {
    "name": "Membership"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "serial": {
      "type": "string"
    },
    "expiry": {
      "type": "datetime"
    },
    "membership_transactions": {
      "collection": "membership-transactions"
    },
    "qrcode_url": {
      "type": "string"
    },
    "package": {
      "type": "integer"
    },
    "limit": {
      "type": "biginteger",
      "default": "0"
    }
  }
}
