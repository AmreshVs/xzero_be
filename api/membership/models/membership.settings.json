{
  "kind": "collectionType",
  "collectionName": "memberships",
  "info": {
    "name": "Membership"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "via": "membership",
      "plugin": "users-permissions",
      "model": "user"
    },
    "serial": {
      "type": "string"
    },
    "expiry": {
      "type": "datetime"
    },
    "membership_transactions": {
      "collection": "membership-transactions"
    },
    "qrcode_url": {
      "type": "string"
    },
    "limit": {
      "type": "biginteger",
      "default": "0"
    },
    "center_check_ins": {
      "collection": "center-check-in",
      "via": "membership"
    },
    "is_gift_generated": {
      "type": "boolean"
    },
    "gift_generated_date": {
      "type": "datetime"
    },
    "package": {
      "model": "membership-plans"
    },
    "remarks": {
      "type": "string"
    }
  }
}
