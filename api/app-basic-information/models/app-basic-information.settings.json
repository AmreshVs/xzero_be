{
  "kind": "singleType",
  "collectionName": "app_basic_informations",
  "info": {
    "name": "App Basic Information"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "app_version": {
      "type": "string"
    },
    "version_check": {
      "type": "boolean",
      "default": false
    },
    "android_app_version": {
      "type": "string"
    },
    "android_version_check": {
      "type": "boolean"
    },
    "iso_app_version": {
      "type": "string"
    },
    "ios_version_check": {
      "type": "boolean"
    },
    "otp_expiry_seconds": {
      "type": "integer",
      "default": 10
    },
    "voucher_queue_expiry_seconds": {
      "type": "integer",
      "default": 10
    },
    "total_downloads": {
      "type": "biginteger",
      "private": true
    },
    "error_logging": {
      "type": "boolean",
      "default": true
    },
    "payment_logging": {
      "type": "boolean",
      "default": true
    },
    "voucher_queue_delete_after": {
      "type": "integer"
    }
  }
}
